    "@storybook/react": "^3.4.10",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^9.0.0",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-styled-components": "^1.6.4",
    "babel-plugin-transform-react-require": "^1.0.1",
    "babel-preset-env": "^1.7.0",
    "react": "^16.5.0",
    "react-dom": "^16.5.0",
    "rimraf": "^2.6.2",
    "standard": "^12.0.1"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "lodash": "^4.17.10",
    "open-color": "^1.6.3",
    "prop-types": "^15.6.2",
    "shevyjs": "^1.1.0",
